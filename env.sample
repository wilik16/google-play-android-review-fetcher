# Google Play Store package name
PACKAGE_NAME=com.example.app

# Google Chat webhook URL for notifications
WEBHOOK_URL=https://chat.googleapis.com/v1/spaces/XXXXX/messages?key=YOUR_KEY&token=YOUR_TOKEN

# Path to Google service account credentials
KEY_PATH=./service-account.json

# CSV file to store reviews
REVIEWS_CSV=reviews.csv

# Directory for log files
LOG_DIR=./logs

# Test mode (true = only log webhook messages, false = actually send them)
TEST_MODE=true 